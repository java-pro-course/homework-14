import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
       Scanner in =new Scanner(System.in);
        int n = in.nextInt();
        int a = in.nextInt();
        int c = a - n;
        if (c >= 0) {
            System.out.println(a);
        }
        else {
            System.out.println(n);

            }
            }

    }
