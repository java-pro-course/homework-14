import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
       Scanner in =new Scanner(System.in);
        int n = in.nextInt();
        int b = n%2;
        if (b == 0) {
            System.out.println(true);
        }
        else {
            System.out.println(false);
        }
            }
    }
